{
  "name": "react-redux-todolist",
  "version": "1.0.0",
  "description": "Пример приложения 'Список дел' на React и Redux",
  "author": "monochromer@mail.ru",
  "homepage": "https://github.com/monochromer/react-redux-todolist#readme",
  "keywords": [
    "react",
    "redux",
    "webpack",
    "todo"
  ],
  "engines": {
    "node": ">=8.4.x",
    "npm": ">=5.4.x"
  },
  "scripts": {
    "start": "npm run server",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --hot --watch --inline --colors",
    "dev-build": "cross-env NODE_ENV=development webpack --colors",
    "prod-build": "cross-env NODE_ENV=production webpack --colors",
    "build": "rm -rf build && npm run prod-build && npm run storybook:build && npm run build-pwa",
    "server": "node server.js",
    "deploy": "gh-pages -d public",
    "storybook:dev": "start-storybook -p 9001 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -o public/storybook",
    "build-pwa": "node scripts/build-pwa.js",
    "test": "jest --config jest.config.json",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/monochromer/react-redux-todolist.git"
  },
  "bugs": {
    "url": "https://github.com/monochromer/react-redux-todolist/issues"
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "babel-runtime": "6.26.0",
    "classnames": "2.2.5",
    "compression": "1.7.2",
    "express": "4.16.3",
    "helmet": "3.12.0",
    "normalize.css": "8.0.0",
    "perfect-scrollbar": "1.3.0",
    "prop-types": "15.6.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-flip-move": "3.0.1",
    "react-helmet": "5.2.0",
    "react-redux": "5.0.7",
    "redux": "3.7.2",
    "reselect": "3.0.1"
  },
  "devDependencies": {
    "@storybook/addon-notes": "3.3.15",
    "@storybook/react": "3.3.15",
    "autoprefixer": "8.1.0",
    "babel-core": "6.26.0",
    "babel-jest": "23.0.0-alpha.0",
    "babel-loader": "7.1.4",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-minify": "0.5.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "commander": "2.15.0",
    "common-tags": "1.7.2",
    "cross-env": "5.1.4",
    "css-loader": "0.28.10",
    "cssnano": "3.10.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "1.1.11",
    "gh-pages": "1.1.0",
    "html-webpack-plugin": "3.0.6",
    "identity-obj-proxy": "3.0.0",
    "image-webpack-loader": "4.2.0",
    "jest": "22.4.2",
    "postcss-loader": "2.1.1",
    "react-hot-loader": "4.0.0",
    "style-loader": "0.20.3",
    "svg-url-loader": "2.3.2",
    "url-loader": "1.0.1",
    "webpack": "4.1.1",
    "webpack-cli": "2.0.12",
    "webpack-dev-server": "3.1.1",
    "webpack-manifest-plugin": "2.0.0-rc.2"
  }
}
