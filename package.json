{
  "name": "react-redux-todolist",
  "version": "0.1.0",
  "description": "Пример приложения 'Список дел' на React и Redux",
  "author": "monochromer@mail.ru",
  "homepage": "https://github.com/monochromer/react-redux-todolist#readme",
  "keywords": [
    "react",
    "redux",
    "webpack",
    "todo"
  ],
  "engines": {
    "node": ">=8.0",
    "npm": ">=5.4"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot --watch --inline --colors",
    "dev-build": "cross-env NODE_ENV=development webpack --colors",
    "prod-build": "cross-env NODE_ENV=production webpack --colors",
    "build": "rm -rf build && npm run prod-build && npm run storybook:build",
    "server": "static public --spa --gzip",
    "deploy": "gh-pages -d public",
    "storybook:dev": "start-storybook -p 9001 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -o public/storybook",
    "test": "jest --config jest.config.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/monochromer/react-redux-todolist.git"
  },
  "bugs": {
    "url": "https://github.com/monochromer/react-redux-todolist/issues"
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "babel-runtime": "6.26.0",
    "classnames": "2.2.5",
    "node-static": "0.7.10",
    "normalize.css": "7.0.0",
    "perfect-scrollbar": "1.3.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-flip-move": "3.0.0",
    "react-redux": "5.0.6",
    "redux": "3.7.2",
    "reselect": "3.0.1"
  },
  "devDependencies": {
    "@storybook/addon-notes": "3.3.10",
    "@storybook/react": "3.3.10",
    "autoprefixer": "7.2.5",
    "babel-core": "6.26.0",
    "babel-jest": "22.1.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "cross-env": "5.1.3",
    "css-loader": "0.28.8",
    "cssnano": "3.10.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.6",
    "gh-pages": "1.1.0",
    "html-webpack-plugin": "2.30.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "22.1.4",
    "postcss-loader": "2.0.10",
    "react-hot-loader": "4.0.0-beta.14",
    "style-loader": "0.19.1",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.11.0",
    "webpack-manifest-plugin": "1.3.2"
  }
}
