{
  "name": "react-redux-todolist",
  "version": "1.0.1",
  "description": "Пример приложения 'Список дел' на React и Redux",
  "author": "monochromer@mail.ru",
  "homepage": "https://github.com/monochromer/react-redux-todolist#readme",
  "keywords": [
    "react",
    "redux",
    "webpack",
    "todo"
  ],
  "engines": {
    "node": ">=8.4.x",
    "npm": ">=5.4.x"
  },
  "scripts": {
    "start": "npm run server",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --hot --watch --inline --colors",
    "dev-build": "cross-env NODE_ENV=development webpack --colors",
    "prod-build": "cross-env NODE_ENV=production webpack --colors",
    "build": "rm -rf build && npm run prod-build && npm run storybook:build && npm run build-pwa",
    "server": "node server.js",
    "deploy": "gh-pages -d public",
    "storybook:dev": "start-storybook -p 9001 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -o public/storybook",
    "build-pwa": "node scripts/build-pwa.js",
    "test": "jest --config jest.config.json",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/monochromer/react-redux-todolist.git"
  },
  "bugs": {
    "url": "https://github.com/monochromer/react-redux-todolist/issues"
  },
